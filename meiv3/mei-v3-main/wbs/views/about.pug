head
	script(src="//code.jquery.com/jquery-1.11.1.min.js")
	link#bootstrap-css(href="//maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" rel="stylesheet")
	style
		include ./style.css
	script(src="//maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js")
	script
		include script.js
	title=title
	meta(name="description", content=`Asphalt 9 MEI Database
About US`)
	meta(name="viewport", content=`width=device-width, initial-scale=1`)

#wrapper.animate
	nav.navbar.header-top.fixed-top.navbar-expand-lg.navbar-dark.bg-dark
		span.navbar-toggler-icon.modal-close
		a.navbar-brand(href="/") MEI - Database
		button.navbar-toggler(type="button" data-toggle="collapse" data-target="#navbarText" aria-controls="navbarText" aria-expanded="false" aria-label="Toggle navigation")
			span.navbar-toggler-icon
		#navbarText.collapse.navbar-collapse
			ul.navbar-nav.animate.side-nav
				li.nav-item
					a.nav-link(href="/")
						| Home
						span.sr-only(current)
					each nav in navs
						a.nav-link(href=nav.path)
							| #{nav.name}
							span.sr-only(current)
			ul.navbar-nav.ml-md-auto.d-md-flex
				li.nav-item
					a.nav-link(href="https://discord.gg/KgKMgxd" target="_blank")
						| Discord
						span.sr-only(current)
	.container-fluid
		.row
			.col
				.card
					.card-body
						h5.card-title MEI Creators & Contributors
						p.card-text
							| - Hariix_#0690 (Vincent) & Céline#4132 [Developers] - NOT ACTIVE
							br
							| - Tamamo Kitsune#1248 [Main Contributor/Pack Management] - NOT ACTIVE
							br
							| - finn#2222 [Local Cars Stats Slave] - NOT ACTIVE
							br
							| - PausedRebel#8398 [Main Events Manager] - NOT ACTIVE
							br
							| - Necro Night#5917 & psycho#7759 [IOS Events Managers] - NOT ACTIVE
							br
							| - ASL 소사벌#6319 [ANDROID Events Manager] - RETIRED
							br
							| - RacerBest#9135 [Decals Icons] - NOT ACTIVE
							br
							| - Bullet_Magnit#8681, Red Panda#1017 & ThuHiền#2021 [Bugs Hunters] - NOT ACTIVE
		.row
			.col
				.card
					.card-body
						h5.card-title= `Who use the bot? ${guilds.length} servers does.`
						table.table
							thead
								tr
								th(scope="col") #[strong NAME]
								th(scope="col") #[strong MEMBERS]
							tbody
								each val in guilds
									tr
									th(scope="row")=val.name
									td
										| #{val.members}
								if guilds.length == 0
									tr
									th(scope="row") Ouch